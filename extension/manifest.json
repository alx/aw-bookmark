{
  "manifest_version": 2,
  "name": "AW Bookmark",
  "version": "1.0",
  "description": "Save bookmarks to ActivityWatch with one click",

  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Save to ActivityWatch"
  }
}
